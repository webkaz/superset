{
	"manifest_version": 3,
	"name": "Superset Browser Tools",
	"version": "0.1.0",
	"description": "Browser tools for Superset desktop",
	"permissions": ["activeTab", "scripting"],
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"],
			"run_at": "document_idle"
		}
	]
}
